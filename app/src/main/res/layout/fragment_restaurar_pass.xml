<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.RestaurarPass">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="506dp"
            android:layout_height="894dp"
            android:scaleType="centerCrop"
            android:src="@drawable/aqpnoche"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <FrameLayout
            android:layout_width="347dp"
            android:layout_height="551dp"
            android:layout_marginStart="29dp"
            android:layout_marginTop="131dp"
            android:background="#FFFEFE"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </FrameLayout>

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="147dp"
            android:layout_height="147dp"
            android:layout_marginStart="135dp"
            android:layout_marginTop="54dp"
            android:src="@drawable/logo_app_3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="127dp"
            android:layout_marginTop="219dp"
            android:text="Restaura tu"
            android:textColorLink="#000000"
            android:textSize="30sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="132dp"
            android:layout_marginTop="259dp"
            android:text="contraseña"
            android:textColorLink="#000000"
            android:textSize="30sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="90dp"
            android:layout_marginTop="316dp"
            android:text="Ingresa tu nueva contraseña, y"
            android:textSize="17sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:layout_marginTop="339dp"
            android:text="considera guardar tu contraseña en"
            android:textSize="17sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="114dp"
            android:layout_marginTop="354dp"
            android:paddingStart="1px"
            android:paddingLeft="1px"
            android:text="en un lugar mas seguro."
            android:textSize="17sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginTop="412dp"
            android:text="Nueva contraseña"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginTop="514dp"
            android:text="COnfirmación COntraseña"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editTextText3"
            android:layout_width="314dp"
            android:layout_height="50dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="447dp"
            android:background="#FCF0BF"
            android:ems="10"
            android:hint="Contraseña"
            android:inputType="text"
            android:padding="2px"
            android:paddingStart="2px"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editTextText"
            android:layout_width="314dp"
            android:layout_height="50dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="547dp"
            android:background="#FCF0BF"
            android:ems="10"
            android:hint="Contraseña"
            android:inputType="text"
            android:padding="2px"
            android:paddingStart="2px"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button2"
            android:layout_width="306dp"
            android:layout_height="51dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="608dp"
            android:background="#FF7F50"
            android:backgroundTint="@color/material_dynamic_secondary20"
            android:text="Restaurar Contraseña"
            android:textColorLink="#FF7F50"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>